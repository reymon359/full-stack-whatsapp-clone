{
  "name": "full-stack-whatsapp-clone-client",
  "version": "1.0.0",
  "description": "Client side from A whatsapp clone made with React, Typescript, GraphQL, and other technologies",
  "private": true,
  "author": "ramon morcillo @reymon359",
  "license": "ISC",
  "scripts": {
    "prestart": "yarn codegen",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "format": "prettier \"**/*.{ts,tsx,css,graphql}\" --write",
    "codegen": "graphql-codegen",
    "size": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "@graphql-codegen/add": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-operations": "1.13.5",
    "@graphql-codegen/typescript-react-apollo": "1.13.5",
    "@material-ui/core": "4.9.13",
    "@material-ui/icons": "4.9.1",
    "@testing-library/jest-dom": "5.3.0",
    "@testing-library/react": "10.0.2",
    "@testing-library/user-event": "10.0.1",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.8",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.9",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "apollo-link-ws": "1.0.20",
    "apollo-utilities": "1.3.4",
    "date-fns": "2.14.0",
    "graphql": "15.0.0",
    "graphql-tag": "2.10.3",
    "history": "4.10.1",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "jest-fetch-mock": "3.0.3",
    "moment": "2.25.3",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-router-transition": "2.0.0",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.0",
    "subscriptions-transport-ws": "0.9.16",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
    "apollo-link-mock": "1.0.1",
    "source-map-explorer": "2.4.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(react-router-transition)/)"
    ]
  }
}
